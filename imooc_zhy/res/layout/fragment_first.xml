<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#99ecb2"
    android:orientation="vertical" >

    <Button
        android:id="@+id/btn_customview"
        style="@style/btn_style"
        android:text="CustomView" />

    <Button
        android:id="@+id/btn_rolateanim"
        style="@style/btn_style"
        android:text="RolateAnim" />

    <Button
        android:id="@+id/btn_chouJiang"
        style="@style/btn_style"
        android:text="ChouJiang" />

    <Button
        android:id="@+id/btn_CircleMenu"
        style="@style/btn_style"
        android:text="CircleMenu" />

    <Button
        android:id="@+id/btn_SlidingMenu"
        style="@style/btn_style"
        android:text="BinarySlidingMenu" />

    <Button
        android:id="@+id/btn_CustomPargressBar"
        style="@style/btn_style"
        android:text="CustomPargressBar" />

    <Button
        android:id="@+id/btn_GestureLock"
        style="@style/btn_style"
        android:text="GestureLock" />

    <Button
        android:id="@+id/btn_SlidingPanelLayout"
        style="@style/btn_style"
        android:text="SlidingPanelLayout" />

    <Button
        android:id="@+id/btn_ViewDragHelper"
        style="@style/btn_style"
